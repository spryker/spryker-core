{
    "name": "spryker/asset",
    "type": "library",
    "description": "Asset module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/event-behavior": "^1.23.0",
        "spryker/kernel": "^3.30.0",
        "spryker/message-broker-extension": "^1.0.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/store": "^1.12.0",
        "spryker/store-reference": "^1.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/ramsey-uuid": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/event": "If you want to use Event plugins, minimum required version: 2.1.0"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\Asset\\Helper\\": "tests/SprykerTest/Shared/Asset/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
