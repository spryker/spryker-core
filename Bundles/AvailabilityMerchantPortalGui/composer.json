{
  "name": "spryker/availability-merchant-portal-gui",
  "type": "library",
  "description": "AvailabilityMerchantPortalGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/availability": "^9.8.0",
    "spryker/decimal-object": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant-stock": "^0.2.2",
    "spryker/merchant-user": "^0.6.0",
    "spryker/product-merchant-portal-gui-extension": "^0.1.1",
    "spryker/transfer": "^3.25.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
