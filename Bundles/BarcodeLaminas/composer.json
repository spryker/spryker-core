{
    "name": "spryker/barcode-laminas",
    "type": "library",
    "description": "BarcodeLaminas module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/barcode-extension": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/kernel": "*",
        "spryker/laminas": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/kernel": "If you want to use Code128BarcodeGeneratorPlugin.",
        "spryker/laminas": "If you want to use Code128BarcodeGeneratorPlugin."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
