{
  "name": "spryker/cart-variant",
  "description": "CartVariant module",
  "license": "proprietary",
  "require": {
    "spryker/availability": "^5.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/product": "^5.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/",
      "ClientUnit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
