{
  "name": "spryker/carts-rest-api",
  "type": "library",
  "description": "CartsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cart": "^5.1.0",
    "spryker/glue-application": "^1.0.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.22.0",
    "spryker/multi-cart": "^1.1.0",
    "spryker/persistent-cart": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/quote": "^2.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/zed-request": "^3.4.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/uuid-behavior": "Used to generate UUIDs for the cart entities."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
