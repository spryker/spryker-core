{
  "name": "spryker/category-exporter",
  "type": "library",
  "description": "CategoryExporter module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/category": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/key-builder": "^1.0.0",
    "spryker/storage": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
