{% set chartType = 'scatter' %}
<script>/*<![CDATA[*/
    var chartData = [];

    {% for trace in data.traces %}
        chartData.push({
            name: '{{ trace.name }}',
            type: '{{ chartType }}',
            x: {{ trace.labels | json_encode | raw }},
            y: {{ trace.values | json_encode | raw }}
        });
    {% endfor %}
/*]]>*/</script>

{% include '@Chart/_partials/plotly-chart.twig' with {
    chartType: chartType,
    layout: layout,
    data: data
} %}
