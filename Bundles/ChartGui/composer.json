{
  "name": "spryker/chart-gui",
  "type": "library",
  "description": "ChartGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/kernel": "^3.30.0",
    "spryker/twig-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/chart": "*",
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/chart": "If you want to use Chart Plugins. Minimum required version: 1.0.1.",
    "spryker/container": "If you want to use Twig Plugins",
    "spryker/gui": "If you want to use Gui twig functions. Minimum required version: 3.26.0.",
    "spryker/twig": "If you want to use Twig Plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
