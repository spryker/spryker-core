{
    "name": "spryker/click-and-collect-example",
    "type": "library",
    "description": "ClickAndCollectExample module",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "spryker/availability": "^9.20.0",
        "spryker/checkout-rest-api-extension": "^1.5.0",
        "spryker/currency": "^3.0.0 || ^4.0.0",
        "spryker/glue-application-extension": "^1.10.0",
        "spryker/kernel": "^3.30.0",
        "spryker/merchant-user": "^1.0.0",
        "spryker/price-product-offer": "^1.0.0",
        "spryker/product-offer": "^1.0.0",
        "spryker/product-offer-service-point": "^1.0.0",
        "spryker/product-offer-service-point-availability-calculator-storage-extension": "^1.0.0",
        "spryker/product-offer-shipment-type": "^1.0.0",
        "spryker/product-offer-stock": "^1.0.0",
        "spryker/product-offer-storage": "^1.0.0",
        "spryker/propel-orm": "^1.18.0",
        "spryker/service-point": "^1.0.0",
        "spryker/service-point-cart-extension": "^1.0.0",
        "spryker/shipment": "^8.18.0",
        "spryker/shipment-type": "^1.0.0",
        "spryker/shipment-type-service-point": "^1.0.0",
        "spryker/shipment-type-storage-extension": "^1.0.0",
        "spryker/store": "^1.19.0",
        "spryker/symfony": "^3.7.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/decimal-object": "*",
        "spryker/merchant": "*",
        "spryker/price-product": "*",
        "spryker/product": "*",
        "spryker/propel": "*",
        "spryker/stock": "*",
        "spryker/testify": "*",
        "spryker/user": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
