{
  "name": "spryker/cms",
  "description": "Cms module",
  "license": "proprietary",
  "require": {
    "spryker/category": "^3.0.0",
    "spryker/glossary": "^3.1.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/key-builder": "^1.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/storage": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/touch": "^3.0.0",
    "spryker/url": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cms-block": "If you want to have separate content blocks on your pages."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
