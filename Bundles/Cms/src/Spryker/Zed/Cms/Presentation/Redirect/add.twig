{% extends '@Cms/Layout/layout.twig' %}

{% set widget_title = 'Add a CMS Redirect' %}

{% block head_title %}
    {{ widget_title | trans }}
{% endblock %}
{% block section_title %}
    {{ widget_title | trans }}
{% endblock %}

{% block action %}
    {{ backActionButton('/cms/redirect', 'Back to CMS' | trans) }}
{% endblock %}

{% block content %}

    {% embed '@Gui/Partials/widget.twig' %}

        {% block widget_content %}

            {{ form_start(form) }}

                {{ form_widget(form) }}

                <input type="submit" class="btn btn-primary safe-submit" value="{{ 'Save' | trans }}" />

            {{ form_end(form) }}

        {% endblock %}

    {% endembed %}

{% endblock %}
