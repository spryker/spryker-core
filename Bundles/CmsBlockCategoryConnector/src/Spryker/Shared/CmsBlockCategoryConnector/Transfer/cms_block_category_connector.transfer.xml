<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CmsBlock">
        <property name="idCmsBlock" type="int"/>
        <property name="idCategories" type="array" singular="idCategories"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Category">
        <property name="idCategory" type="int"/>
        <property name="idCmsBlocks" type="array" singular="idCmsBlocks"/>
        <property name="fkCategoryTemplate" type="int"/>
    </transfer>

    <transfer name="CmsBlockCategoryPosition">
        <property name="idCmsBlockCategoryPosition" type="int"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
    </transfer>

</transfers>
