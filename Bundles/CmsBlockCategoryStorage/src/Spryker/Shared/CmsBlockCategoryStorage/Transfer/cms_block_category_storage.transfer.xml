<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CmsBlockCategories">
        <property name="idCategory" type="int"/>
        <property name="cmsBlockCategories" singular="cmsBlockCategory" type="CmsBlockCategory[]"/>
    </transfer>

    <transfer name="CmsBlockCategory">
        <property name="position" type="string"/>
        <property name="blockNames" type="array" singular="blockNames"/>
        <property name="blockKeys" type="array" singular="blockKeys"/>
    </transfer>

    <transfer name="CmsBlock">
        <property name="key" type="string"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="store" type="string"/>
        <property name="locale" type="string"/>
    </transfer>

    <transfer name="CmsBlockRequest">
        <property name="category" type="int"/>
        <property name="position" type="string"/>
    </transfer>

</transfers>
