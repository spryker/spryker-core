{
  "name": "spryker/cms-block-product-connector",
  "description": "CmsBlockProductConnector module",
  "license": "proprietary",
  "require": {
    "spryker/cms-block": "^1.2.0",
    "spryker/collector": "^5.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/product": "^5.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/touch": "^3.0.0",
    "spryker/util-data-reader": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/cms-block-gui": "*",
    "spryker/product-management": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cms-block-gui": "^1.0.0",
    "spryker/product-management": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
