<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CmsContentWidgetConfigurationList">
        <property name="cmsContentWidgetConfigurationList" type="CmsContentWidgetConfiguration[]" singular="cmsContentWidgetConfiguration"/>
    </transfer>

    <transfer name="CmsContentWidgetConfiguration">
        <property name="templates" type="array" singular="template"/>
        <property name="usageInformation" type="string"/>
        <property name="functionName" type="string"/>
    </transfer>

    <transfer name="CmsContentWidgetFunctions">
        <property name="CmsContentWidgetFunctionList" type="CmsContentWidgetFunction[]" singular="cmsContentWidgetFunction"/>
    </transfer>

    <transfer name="CmsContentWidgetFunction">
        <property name="functionName" type="string"/>
        <property name="parameters" type="array" singular="parameter"/>
    </transfer>

    <transfer name="Locale">
        <property name="localeName" type="string"/>
    </transfer>

</transfers>
