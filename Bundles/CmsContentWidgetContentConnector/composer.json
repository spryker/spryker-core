{
  "name": "spryker/cms-content-widget-content-connector",
  "type": "library",
  "description": "CmsContentWidgetContentConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/content": "^2.2.0",
    "spryker/kernel": "^3.33.0"
  },
  "require-dev": {
    "spryker/cms-content-widget": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cms-content-widget": "Use this module when you want to use the Cms Content Widget."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
