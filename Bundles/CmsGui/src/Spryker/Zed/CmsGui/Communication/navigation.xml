<?xml version="1.0"?>
<config>
    <cms-gui>
        <label>CMS</label>
        <title>CMS</title>
        <icon>fa-files-o</icon>
        <pages>
            <page>
                <label>Pages</label>
                <title>Pages</title>
                <bundle>cms-gui</bundle>
                <controller>list-page</controller>
                <action>index</action>
                <pages>
                    <version>
                        <label>Version</label>
                        <title>Version</title>
                        <bundle>cms-gui</bundle>
                        <controller>version-page</controller>
                        <action>history</action>
                        <visible>0</visible>
                    </version>
                    <view-page>
                        <label>View CMS Page</label>
                        <title>View CMS Page</title>
                        <bundle>cms-gui</bundle>
                        <controller>view-page</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </view-page>
                    <edit-page>
                        <label>Edit CMS Page</label>
                        <title>Edit CMS Page</title>
                        <bundle>cms-gui</bundle>
                        <controller>edit-page</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </edit-page>
                    <create-page>
                        <label>Create new CMS Page</label>
                        <title>Create new CMS Page</title>
                        <bundle>cms-gui</bundle>
                        <controller>create-page</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </create-page>
                    <edit-placeholder>
                        <label>Edit CMS Placeholders</label>
                        <title>Edit CMS Placeholders</title>
                        <bundle>cms-gui</bundle>
                        <controller>create-glossary</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </edit-placeholder>
                </pages>
            </page>
            <redirects>
                <label>Redirects</label>
                <title>Redirects</title>
                <bundle>cms</bundle>
                <controller>redirect</controller>
                <action>index</action>
            </redirects>
        </pages>
    </cms-gui>
</config>
