{
    "name": "spryker/cms-pages-content-banners-resource-relationship",
    "type": "library",
    "description": "CmsPagesContentBannersResourceRelationship module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/cms-storage": "^2.4.0",
        "spryker/content-banners-rest-api": "^2.2.0",
        "spryker/glue-application": "^1.8.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.33.0",
        "spryker/store": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
