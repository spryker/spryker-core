<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CmsSlotTemplateConfiguration">
        <property name="conditions" type="string[]" singular="condition"/>
    </transfer>

    <transfer name="CmsSlotBlockCondition">
        <property name="all" type="bool"/>
        <property name="categoryIds" type="int[]" singular="idCategory"/>
    </transfer>

    <transfer name="Locale">
    </transfer>

    <transfer name="CategoryCollection">
        <property name="categories" type="Category[]" singular="category"/>
    </transfer>

    <transfer name="Category">
        <property name="name" type="string"/>
        <property name="idCategory" type="int"/>
    </transfer>
</transfers>
