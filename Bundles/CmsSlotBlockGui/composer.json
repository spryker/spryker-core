{
  "name": "spryker/cms-slot-block-gui",
  "type": "library",
  "description": "CmsSlotBlockGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/cms-block": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "spryker/cms-slot": "^1.0.0",
    "spryker/cms-slot-block": "^1.0.0",
    "spryker/cms-slot-block-gui-extension": "^1.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.8.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
