{
    "name": "spryker/comment-sales-connector",
    "type": "library",
    "description": "CommentSalesConnector module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/comment": "^1.7.0",
        "spryker/gui": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/sales-extension": "^1.3.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
