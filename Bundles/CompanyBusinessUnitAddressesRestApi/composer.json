{
  "name": "spryker/company-business-unit-addresses-rest-api",
  "type": "library",
  "description": "CompanyBusinessUnitAddressesRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/checkout-rest-api-extension": "^1.5.0",
    "spryker/company-business-units-rest-api-extension": "^1.0.0",
    "spryker/company-unit-address": "^1.8.0",
    "spryker/glue-application": "^1.15.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/symfony": "^3.0.0",
    "spryker/uuid-behavior": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/auth-rest-api": "If you want to authorize and authenticate customers via access tokens."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
