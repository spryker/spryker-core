{
    "name": "spryker/company-gui-extension",
    "type": "library",
    "description": "CompanyGuiExtension module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/gui": "^3.13.0",
        "spryker/symfony": "^3.0.0"
    },
    "suggest": {
        "spryker/company-gui": "If you want to provide plugins to CompanyGui module",
        "spryker/gui": "If you want to use CompanyGuiExtension module plugin interfaces which used Gui transfer object.",
        "spryker/symfony": "If you want to use CompanyGui plugins"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
