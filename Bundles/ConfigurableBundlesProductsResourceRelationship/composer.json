{
  "name": "spryker/configurable-bundles-products-resource-relationship",
  "description": "ConfigurableBundlesProductsResourceRelationship module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/catalog": "^5.5.0",
    "spryker/glue-application": "^1.9.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/products-rest-api": "2.11.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
