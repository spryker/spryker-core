{
    "name": "spryker/configurable-bundles-rest-api",
    "type": "library",
    "description": "ConfigurableBundlesRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/configurable-bundle": "^2.0.0",
        "spryker/configurable-bundle-page-search": "^1.0.0",
        "spryker/configurable-bundle-storage": "^2.5.0",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/glue-application": "^1.9.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/orders-rest-api-extension": "^1.0.0",
        "spryker/quote-requests-rest-api-extension": "^1.0.0",
        "spryker/symfony": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
