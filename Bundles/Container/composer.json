{
  "name": "spryker/container",
  "type": "library",
  "description": "Container module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "psr/container": "^1.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    },
    "psr-0": {
      "Pimple": "src/Spryker"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "conflict": {
    "spryker/pimple": "*",
    "pimple/pimple": "*",
    "silex/silex": "*"
  }
}
