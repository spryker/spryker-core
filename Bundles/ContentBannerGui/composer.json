{
    "name": "spryker/content-banner-gui",
    "type": "library",
    "description": "ContentBannerGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/content-banner": "^2.1.0",
        "spryker/content-gui-extension": "^1.1.0",
        "spryker/kernel": "^3.30.0",
        "spryker/symfony": "^3.1.5",
        "spryker/util-encoding": "^2.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
