{
    "name": "spryker/content-file",
    "type": "library",
    "description": "ContentFile module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/content-storage": "^2.0.0",
        "spryker/kernel": "^3.33.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/storage": "^3.8.0",
        "spryker/storage-redis": "^1.0.0",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
