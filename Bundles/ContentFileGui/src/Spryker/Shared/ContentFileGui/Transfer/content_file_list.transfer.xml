<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="ContentFileListTerm">
        <property name="fileIds" type="int[]"/>
    </transfer>

    <transfer name="LocalizedContent">
        <property name="fkLocale" type="int"/>
    </transfer>

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
    </transfer>

    <transfer name="ContentValidationResponse">
        <property name="isSuccess" type="bool"/>
        <property name="parameterMessages" type="ContentParameterMessage[]"/>
    </transfer>

    <transfer name="ContentParameterMessage">
        <property name="parameter" type="string"/>
        <property name="messages" type="Message[]"/>
    </transfer>

    <transfer name="Message">
        <property name="value" type="string"/>
        <property name="parameters" type="array"/>
    </transfer>

    <transfer name="ContentWidgetTemplate">
        <property name="identifier" type="string"/>
        <property name="name" type="string"/>
    </transfer>
</transfers>
