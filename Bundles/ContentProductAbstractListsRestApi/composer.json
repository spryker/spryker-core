{
    "name": "spryker/content-product-abstract-lists-rest-api",
    "type": "library",
    "description": "ContentProductAbstractListsRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/content-product": "^1.1.0",
        "spryker/glue-application": "^1.15.0",
        "spryker/glue-application-extension": "^1.2.0",
        "spryker/kernel": "^3.32.0",
        "spryker/products-rest-api": "^2.10.0",
        "spryker/store": "^1.0.0",
        "spryker/symfony": "^3.2.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
