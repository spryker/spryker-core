{
  "name": "spryker/content-product-set-data-import",
  "type": "library",
  "description": "ContentProductSetDataImport module",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "spryker/content": "^2.1.0",
    "spryker/data-import": "^1.5.0",
    "spryker/kernel": "^3.33.0",
    "spryker/product-set": "^1.4.0",
    "spryker/util-encoding": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\ContentProductSetDataImport\\Helper\\": "tests/SprykerTest/Zed/ContentProductSetDataImport/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
