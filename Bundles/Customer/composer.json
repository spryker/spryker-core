{
  "name": "spryker/customer",
  "description": "Customer module",
  "license": "proprietary",
  "require": {
    "spryker/country": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/mail": "^4.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sequence-number": "^3.0.0",
    "spryker/session": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/assertion": "*",
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/console": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins.",
    "spryker/sales": "If you want customer information in sales."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\Customer\\": "tests/SprykerTest/Shared/Customer/_support/",
      "SprykerTest\\Zed\\Customer\\": "tests/SprykerTest/Zed/Customer/_support/",
      "Customer\\Module\\": "tests/_support/Module/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    },
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
