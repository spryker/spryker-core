{
    "name": "spryker/customer-access-permission",
    "type": "library",
    "description": "CustomerAccessPermission module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/customer": "^7.0.0",
        "spryker/customer-access-storage": "^1.0.0",
        "spryker/customer-extension": "^1.2.0",
        "spryker/kernel": "^3.30.0",
        "spryker/permission-extension": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
