<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestCustomerAccessAttributes">
        <property name="resourceTypes" type="string[]" singular="resourceType"/>
    </transfer>

    <transfer name="RestErrorCollection">
        <property name="restErrors" type="RestErrorMessage[]" singular="restError"/>
    </transfer>

    <transfer name="RestErrorMessage">
        <property name="status" type="int"/>
        <property name="code" type="string"/>
        <property name="detail" type="string"/>
    </transfer>

    <transfer name="CustomerAccess">
        <property name="ContentTypeAccess" type="ContentTypeAccess[]" singular="ContentTypeAccess"/>
    </transfer>

    <transfer name="ContentTypeAccess">
        <property name="contentType" type="string"/>
        <property name="isRestricted" type="bool"/>
    </transfer>

</transfers>
