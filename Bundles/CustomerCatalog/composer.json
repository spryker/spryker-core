{
  "name": "spryker/customer-catalog",
  "type": "library",
  "description": "CustomerCatalog module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/customer": "^7.0.0",
    "spryker/kernel": "^3.30.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/elastica": "*",
    "spryker/search": "*"
  },
  "suggest": {
    "spryker/search": "If you want to use Search plugins, minimum required version: 7.0.0",
    "spryker/elastica": "If you want to use Elastica plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
