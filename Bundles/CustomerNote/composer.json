{
  "name": "spryker/customer-note",
  "description": "CustomerNote module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/customer": "^7.7.0",
    "spryker/kernel": "^3.0.0",
    "spryker/user": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/assertion": "*",
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\CustomerNote\\Helper\\": "tests/SprykerTest/Shared/CustomerNote/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
