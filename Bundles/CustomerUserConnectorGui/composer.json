{
  "name": "spryker/customer-user-connector-gui",
  "type": "library",
  "description": "CustomerUserConnectorGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/customer": "^6.3.0 || ^7.0.0",
    "spryker/customer-user-connector": "^1.0.0",
    "spryker/gui": "^3.10.0",
    "spryker/kernel": "^3.30.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0 || ^4.0.0",
    "spryker/user": "^3.0.0",
    "spryker/user-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/util-text": "*"
  },
  "suggest": {
    "spryker/util-text": "If you want to use the UsersTableExpanderPlugin"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
