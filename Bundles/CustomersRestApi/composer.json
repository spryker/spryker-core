{
  "name": "spryker/customers-rest-api",
  "description": "CustomersRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/customer": "^7.0.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/session": "^3.2.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/glue-application": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/glue-application": "If you want to use Glue infrastructure."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
