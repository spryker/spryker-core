{
  "name": "spryker/dummy-payment",
  "description": "DummyPayment bundle",
  "license": "proprietary",
  "require": {
    "spryker/checkout": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/oms": "^6.0.0 || ^7.0.0",
    "spryker/payment": "^3.0.0",
    "spryker/refund": "^4.0.0 || ^5.0.0",
    "spryker/sales": "^5.0.0 || ^6.0.0",
    "spryker/step-engine": "^2.0.0 || ^3.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/propel-orm": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Config\\Module\\": "vendor/spryker/config/tests/_support/Module",
      "Transfer\\Module\\": "vendor/spryker/transfer/tests/_support/Module",
      "Propel\\Module\\": "vendor/spryker/propel/tests/_support/Module",
      "Testify\\Module\\": "vendor/spryker/testify/tests/_support/Module"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
