<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 https://static.spryker.com/transfer-01.xsd">
    <transfer name="MerchantFileImport" strict="true">
        <property name="idMerchantFileImport" type="int"/>
        <property name="fkMerchantFile" type="int"/>
        <property name="uuid" type="string"/>
        <property name="entityType" type="string"/>
        <property name="status" type="string"/>
        <property name="processedEntitiesCount" type="int"/>
        <property name="failedEntitiesCount" type="int"/>
        <property name="errors" type="string"/>
        <property name="createdAt" type="string"/>
        <property name="startedAt" type="string"/>
        <property name="finishedAt" type="string"/>
        <property name="merchantFile" type="MerchantFile"/>
    </transfer>

    <transfer name="MerchantFileImportCollection" strict="true">
        <property name="merchantFileImports" type="MerchantFileImport[]" singular="merchantFileImport"/>
        <property name="pagination" type="Pagination"/>
    </transfer>

    <transfer name="MerchantFileImportResponse" strict="true">
        <property name="merchantFileImport" type="MerchantFileImport"/>
        <property name="isSuccessful" type="bool"/>
        <property name="messages" type="Message[]" singular="message"/>
    </transfer>

    <transfer name="MerchantFileImportTableCriteria" strict="true">
        <property name="filterCreatedAt" type="CriteriaRangeFilter" strict="true"/>
        <property name="filterEntityTypes" type="string[]" singular="filterEntityType" strict="true"/>
        <property name="filterStatuses" type="string[]" singular="filterStatus" strict="true"/>
        <property name="searchTerm" type="string" strict="true"/>
        <property name="page" type="int" strict="true"/>
        <property name="pageSize" type="int" strict="true"/>
        <property name="orderBy" type="string" strict="true"/>
        <property name="orderDirection" type="string" strict="true"/>
    </transfer>

    <transfer name="MerchantFileImportCriteria" strict="true">
        <property name="merchantFileImportConditions" type="MerchantFileImportConditions"/>
        <property name="limit" type="int"/>
    </transfer>

    <transfer name="MerchantFileImportConditions" strict="true">
        <property name="merchantFileImportIds" type="int[]" singular="idMerchantFileImport"/>
        <property name="uuids" type="string[]" singular="uuid"/>
        <property name="merchantFileIds" type="int[]" singular="idMerchantFile"/>
        <property name="entityTypes" type="string[]" singular="entityType"/>
        <property name="statuses" type="string[]" singular="status"/>
    </transfer>

    <transfer name="MerchantFile" strict="true">
        <property name="merchantFileImport" type="MerchantFileImport"/>
        <property name="uploadedUrl" type="string"/>
        <property name="fileSystemName" type="string"/>
        <property name="fkMerchant" type="int"/>
        <property name="originalFileName" type="string"/>
        <property name="contentType" type="string"/>
        <property name="type" type="string"/>
        <property name="size" type="int"/>
        <property name="realPath" type="string"/>
        <property name="content" type="string"/>
        <property name="uuid" type="string"/>
        <property name="user" type="User"/>
        <property name="idMerchantFile" type="int"/>
    </transfer>

    <transfer name="DataImporterConfigurationContext" strict="true">
        <property name="idMerchant" type="int"/>
    </transfer>

    <transfer name="DataImporterReport">
        <property name="isSuccess" type="bool"/>
        <property name="importedDataSetCount" type="int"/>
        <property name="dataImporterReports" type="DataImporterReport[]" singular="dataImporterReport"/>
        <property name="messages" type="DataImporterReportMessage[]" singular="message"/>
    </transfer>

    <transfer name="DataImporterReportMessage">
        <property name="dataSetNumber" type="int" strict="true"/>
        <property name="dataSetIdentifier" type="string" strict="true"/>
        <property name="message" type="string"/>
    </transfer>

    <transfer name="DataImportConfigurationAction">
        <property name="dataEntity" type="string"/>
        <property name="source" type="string"/>
        <property name="fileSystem" type="string" strict="true"/>
        <property name="context" type="DataImporterConfigurationContext" strict="true"/>
    </transfer>

    <transfer name="DataImporterReaderConfiguration">
        <property name="fileName" type="string"/>
        <property name="fileSystem" type="string" strict="true"/>
    </transfer>

    <transfer name="DataImporterConfiguration">
        <property name="importGroup" type="string"/>
        <property name="importType" type="string"/>
        <property name="readerConfiguration" type="DataImporterReaderConfiguration"/>
        <property name="context" type="DataImporterConfigurationContext" strict="true"/>
    </transfer>

    <transfer name="MerchantFileCollection" strict="true">
        <property name="merchantFiles" type="MerchantFile[]" singular="merchantFile"/>
    </transfer>

    <transfer name="MerchantFileCriteria" strict="true">
        <property name="merchantFileConditions" type="MerchantFileConditions"/>
    </transfer>

    <transfer name="MerchantFileConditions" strict="true">
        <property name="merchantFileIds" type="int[]" singular="merchantFileId"/>
        <property name="uuids" type="string[]" singular="uuid"/>
    </transfer>

    <transfer name="Message">
        <property name="message" type="string"/>
        <property name="value" type="string"/>
        <property name="parameters" type="array" singular="parameters"/>
    </transfer>

    <transfer name="ZedUiFormResponse" strict="true">
    </transfer>

    <transfer name="MerchantFileResult" strict="true">
        <property name="isSuccessful" type="bool"/>
        <property name="messages" type="Message[]" singular="message"/>
    </transfer>

    <transfer name="GuiTableDataRequest" strict="true">
        <property name="page" type="int"/>
        <property name="pageSize" type="int"/>
        <property name="orderBy" type="string"/>
        <property name="orderDirection" type="string"/>
    </transfer>

    <transfer name="GuiTableDataResponse">
        <property name="page" type="int" strict="true"/>
        <property name="pageSize" type="int" strict="true"/>
        <property name="total" type="int" strict="true"/>
        <property name="rows" type="GuiTableRowDataResponse[]" singular="row"/>
    </transfer>

    <transfer name="Pagination">
        <property name="page" type="int"/>
        <property name="maxPerPage" type="int"/>
        <property name="nbResults" type="int"/>
        <property name="firstIndex" type="int"/>
        <property name="lastIndex" type="int"/>
        <property name="firstPage" type="int"/>
        <property name="lastPage" type="int"/>
        <property name="nextPage" type="int"/>
        <property name="previousPage" type="int"/>
    </transfer>

    <transfer name="GuiTableRowDataResponse">
        <property name="responseData" type="array" singular="responseData"/>
    </transfer>

    <transfer name="Rule">
        <property name="bundle" type="string"/>
        <property name="controller" type="string"/>
        <property name="type" type="string"/>
        <property name="action" type="string"/>
    </transfer>

    <transfer name="AclEntityMetadataConfig">
        <property name="aclEntityMetadataCollection" type="AclEntityMetadataCollection"/>
    </transfer>

    <transfer name="AclEntityMetadata">
        <property name="entityName" type="string"/>
        <property name="parent" type="AclEntityParentMetadata"/>
        <property name="isSubEntity" type="bool"/>
    </transfer>

    <transfer name="AclEntityParentMetadata">
        <property name="entityName" type="string"/>
    </transfer>

    <transfer name="CriteriaRangeFilter" strict="true">
        <property name="from" type="string"/>
        <property name="to" type="string"/>
    </transfer>

    <transfer name="User">
        <property name="firstName" type="string"/>
        <property name="lastName" type="string"/>
    </transfer>

    <transfer name="GuiTableConfiguration">
    </transfer>

    <transfer name="AclEntityMetadataCollection">
        <property name="collection" type="array" singular="aclEntityMetadata" associative="true"/>
    </transfer>

</transfers>
