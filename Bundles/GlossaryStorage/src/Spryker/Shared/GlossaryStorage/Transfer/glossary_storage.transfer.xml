<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="GlossaryStorage">
        <property name="idGlossaryStorage" type="int"/>
        <property name="fkGlossaryKey" type="int"/>
        <property name="data" type="string"/>
        <property name="glossaryKey" type="GlossaryKey"/>
        <property name="key" type="string"/>
        <property name="locale" type="Locale"/>
    </transfer>

    <transfer name="GlossaryKey">
        <property name="idGlossaryKey" type="int"/>
        <property name="isActive" type="bool"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="locale" type="string"/>
        <property name="key" type="string"/>
        <property name="data" type="string"/>
    </transfer>

    <transfer name="Filter">
        <property name="limit" type="int"/>
        <property name="offset" type="int"/>
        <property name="orderBy" type="string"/>
    </transfer>

    <transfer name="Locale">
    </transfer>
</transfers>
