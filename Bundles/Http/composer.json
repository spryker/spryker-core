{
  "name": "spryker/http",
  "type": "library",
  "description": "Http module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/application-extension": "^1.0.0",
    "spryker/event-dispatcher-extension": "^1.0.0",
    "spryker/http-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.2.0",
    "spryker/symfony": "^3.3.0",
    "spryker/twig": "^3.7.0",
    "spryker/twig-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/event-dispatcher": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use Http plugins.",
    "spryker/event-dispatcher": "If you want to use Http plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
