<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="Merchant">
        <property name="merchantReference" dataBuilderRule="unique()->regexify('MER-[A-Z0-9]{8}')"/>
        <property name="name" dataBuilderRule="lexify('??????????')"/>
        <property name="registrationNumber" dataBuilderRule="lexify('??????????')"/>
        <property name="status" dataBuilderRule="=waiting-for-approval"/>
        <property name="email" dataBuilderRule="unique()->email()"/>
        <property name="isActive" dataBuilderRule="boolean(100)"/>
    </transfer>
</transfers>
