<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="Merchant">
        <property name="merchantKey" dataBuilderRule="word"/>
        <property name="name" dataBuilderRule="word"/>
        <property name="registrationNumber"  dataBuilderRule="word" />
        <property name="status" dataBuilderRule="=waiting_for_approval" />
        <property name="contactPersonTitle" dataBuilderRule="word" />
        <property name="contactPersonFirstName" dataBuilderRule="word" />
        <property name="contactPersonLastName" dataBuilderRule="word" />
        <property name="contactPersonPhone" dataBuilderRule="e164PhoneNumber" />
        <property name="email" dataBuilderRule="email" />
        <property name="addressCollection" type="MerchantAddressCollection" />
    </transfer>

</transfers>
