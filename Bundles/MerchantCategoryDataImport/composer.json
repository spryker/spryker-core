{
  "name": "spryker/merchant-category-data-import",
  "type": "library",
  "description": "MerchantCategoryDataImport module",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "spryker/category": "^4.0.0 || ^5.0.0",
    "spryker/data-import": "^1.11.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-category": "^0.2.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
