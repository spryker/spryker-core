{
    "name": "spryker/merchant-commission",
    "type": "library",
    "description": "MerchantCommission module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/currency": "^4.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-commission-extension": "^1.0.0",
        "spryker/money": "^2.0.0",
        "spryker/propel-orm": "^1.6.0",
        "spryker/rule-engine": "^1.0.0",
        "spryker/rule-engine-extension": "^1.0.0",
        "spryker/store": "^1.19.0",
        "spryker/transfer": "^3.27.0",
        "spryker/uuid-behavior": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\MerchantCommission\\Helper\\": "tests/SprykerTest/Shared/MerchantCommission/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
