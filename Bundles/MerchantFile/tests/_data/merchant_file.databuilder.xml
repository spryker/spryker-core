<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="MerchantFile" strict="true">
        <property name="type" dataBuilderRule="randomElement(['data-import', 'document'])"/>
        <property name="originalFileName" dataBuilderRule="lexify('merchant-file_????-????.csv')"/>
        <property name="uploadedUrl" dataBuilderRule="lexify('/merchant-file-uploads/????????????????.csv')"/>
        <property name="contentType" dataBuilderRule="randomElement(['application/csv', 'text/plain'])"/>
        <property name="size" dataBuilderRule="numberBetween(1000, 10000000)"/>
        <property name="createdAt" dataBuilderRule="date('Y-m-d H:i:s')"/>
        <property name="fileSystemName" dataBuilderRule="='merchant-files'"/>
        <property name="content" dataBuilderRule="text()"/>
    </transfer>
</transfers>
