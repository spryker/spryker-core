{
    "name": "spryker/merchant-opening-hours-data-import",
    "type": "library",
    "description": "MerchantOpeningHoursDataImport module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/data-import": "^1.3.0",
        "spryker/kernel": "^3.33.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-opening-hours": "^1.0.0",
        "spryker/weekday-schedule": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/merchant-data-import": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Zed\\MerchantOpeningHoursDataImport\\Helper\\": "tests/SprykerTest/Zed/MerchantOpeningHoursDataImport/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
