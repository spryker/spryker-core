{
  "name": "spryker/merchant-opening-hours-rest-api",
  "type": "library",
  "description": "MerchantOpeningHoursRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/glossary-storage": "^1.5.0",
    "spryker/glue-application": "^1.9.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-opening-hours-storage": "^0.2.1",
    "spryker/merchant-storage": "^0.1.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
