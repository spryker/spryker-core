<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestMerchantOpeningHoursAttributes">
        <property name="weekdaySchedule" type="RestWeekdaySchedule[]" singular="weekdaySchedule"/>
        <property name="dateSchedule" type="RestDateSchedule[]" singular="dateSchedule"/>
    </transfer>

    <transfer name="RestWeekdaySchedule">
        <property name="day" type="string"/>
        <property name="timeFrom" type="string"/>
        <property name="timeTo" type="string"/>
    </transfer>

    <transfer name="RestDateSchedule">
        <property name="date" type="string"/>
        <property name="timeFrom" type="string"/>
        <property name="timeTo" type="string"/>
        <property name="noteGlossaryKey" type="string"/>
    </transfer>

    <transfer name="MerchantStorage">
        <property name="idMerchant" type="int"/>
        <property name="merchantReference" type="string"/>
    </transfer>

    <transfer name="MerchantOpeningHoursStorage">
        <property name="dateSchedule" type="DateSchedule[]" singular="dateSchedule"/>
        <property name="weekdaySchedule" type="WeekdaySchedule[]" singular="weekdaySchedule"/>
    </transfer>

    <transfer name="RestErrorMessage">
        <property name="status" type="int"/>
        <property name="code" type="string"/>
        <property name="detail" type="string"/>
    </transfer>

    <transfer name="WeekdaySchedule">
        <property name="day" type="string"/>
        <property name="timeFrom" type="string"/>
        <property name="timeTo" type="string"/>
    </transfer>

    <transfer name="DateSchedule">
        <property name="date" type="string"/>
        <property name="timeFrom" type="string"/>
        <property name="timeTo" type="string"/>
        <property name="noteGlossaryKey" type="string"/>
    </transfer>

</transfers>
