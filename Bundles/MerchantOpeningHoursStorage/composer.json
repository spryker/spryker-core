{
  "name": "spryker/merchant-opening-hours-storage",
  "type": "library",
  "description": "MerchantOpeningHoursStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/event-behavior": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-opening-hours": "^0.2.0",
    "spryker/propel-orm": "^1.5.0",
    "spryker/publisher-extension": "^1.0.0",
    "spryker/storage": "^3.0.0",
    "spryker/synchronization": "^1.0.0",
    "spryker/synchronization-behavior": "^1.0.0",
    "spryker/synchronization-extension": "^1.0.0",
    "spryker/util-encoding": "^1.0.0 || ^2.0.0",
    "spryker/weekday-schedule": "^0.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/event": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/event": "If you want to use Event plugins: ^2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
