{
    "name": "spryker/merchant-product-data-import",
    "type": "library",
    "description": "MerchantProductDataImport module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/category": "^5.0.0",
        "spryker/currency": "^4.5.0",
        "spryker/data-import": "^1.29.0",
        "spryker/kernel": "^3.76.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-file-extension": "^0.1.0",
        "spryker/merchant-product": "^1.0.0",
        "spryker/merchant-stock": "^1.0.0",
        "spryker/price-product": "^4.0.0",
        "spryker/product": "^6.0.0",
        "spryker/product-category": "^4.0.0",
        "spryker/product-image": "^3.18.0",
        "spryker/product-search": "^5.23.0",
        "spryker/propel-orm": "^1.21.0",
        "spryker/stock": "^8.13.0",
        "spryker/tax": "^5.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/url": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
