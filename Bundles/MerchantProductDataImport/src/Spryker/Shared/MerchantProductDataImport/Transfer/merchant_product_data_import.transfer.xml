<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="DataImporterReaderConfiguration">
        <property name="fileName" type="string"/>
    </transfer>

    <transfer name="DataImporterConfiguration">
        <property name="readerConfiguration" type="DataImporterReaderConfiguration"/>
        <property name="importType" type="string"/>
    </transfer>

    <transfer name="DataImporterReport">
    </transfer>

    <transfer name="DataImporterConfigurationContext" strict="true">
        <property name="idMerchant" type="int"/>
    </transfer>

    <transfer name="PriceProduct">
        <property name="idProductAbstract" type="int"/>
        <property name="idProduct" type="int"/>
        <property name="idPriceProduct" type="int"/>
        <property name="moneyValue" type="MoneyValue"/>
        <property name="fkPriceType" type="int"/>
        <property name="groupKey" type="string"/>
        <property name="priceTypeName" type="string"/>
    </transfer>

    <transfer name="ProductAbstractStore">
        <property name="productAbstractSku" type="string"/>
        <property name="storeName" type="string"/>
    </transfer>

    <transfer name="ProductImageSet">
        <property name="name" type="string"/>
        <property name="idProductAbstract" type="int"/>
        <property name="productImages" type="ProductImage[]" singular="productImage"/>
        <property name="idProduct" type="int"/>
        <property name="locale" type="Locale"/>
        <property name="productImageSetKey" type="string"/>
    </transfer>

    <transfer name="Locale">
        <property name="name" type="string"/>
        <property name="idLocale" type="int"/>
    </transfer>

    <transfer name="ProductImage">
        <property name="sortOrder" type="int"/>
    </transfer>

    <transfer name="EventEntity">
        <property name="event" type="string"/>
        <property name="id" type="int"/>
        <property name="name" type="string"/>
        <property name="foreignKeys" type="array" singular="foreignKeys"/>
        <property name="modifiedColumns" type="array" singular="modifiedColumns"/>
    </transfer>

    <transfer name="MerchantFileResult" strict="true">
        <property name="isSuccessful" type="bool"/>
        <property name="messages" type="Message[]" singular="message"/>
    </transfer>

    <transfer name="Message">
        <property name="value" type="string"/>
        <property name="parameters" type="array" singular="parameters"/>
    </transfer>

    <transfer name="MoneyValue">
        <property name="idEntity" type="int"/>
        <property name="fkStore" type="int"/>
        <property name="fkCurrency" type="int"/>
        <property name="netAmount" type="int"/>
        <property name="grossAmount" type="int"/>
    </transfer>

    <transfer name="DataImportMerchantFileCollectionResponse" strict="true">
        <property name="dataImportMerchantFiles" type="DataImportMerchantFile[]" singular="dataImportMerchantFile"/>
        <property name="errors" type="Error[]" singular="error"/>
    </transfer>

    <transfer name="DataImportMerchantFile" strict="true">
        <property name="importerType" type="string"/>
        <property name="fileInfo" type="DataImportMerchantFileInfo"/>
    </transfer>

    <transfer name="DataImportMerchantFileInfo" strict="true">
        <property name="content" type="string"/>
        <property name="fileSystemName" type="string"/>
    </transfer>

    <transfer name="Error">
        <property name="message" type="string"/>
        <property name="parameters" type="array" singular="parameter"/>
        <property name="entityIdentifier" type="string"/>
    </transfer>

    <transfer name="DataImportMerchantFileCollectionRequest" strict="true">
        <property name="dataImportMerchantFiles" type="DataImportMerchantFile[]" singular="dataImportMerchantFile"/>
    </transfer>

    <transfer name="MerchantCriteria">
        <property name="withExpanders" type="bool" strict="true"/>
        <property name="merchantReference" type="string"/>
    </transfer>

    <transfer name="Merchant">
        <property name="merchantReference" type="string"/>
        <property name="idMerchant" type="int"/>
    </transfer>

    <transfer name="MerchantCollection">
        <property name="merchants" type="Merchant[]" singular="merchants"/>
    </transfer>

    <transfer name="StockCollection">
        <property name="stocks" type="Stock[]" singular="stock"/>
    </transfer>

    <transfer name="MerchantStockCriteria">
        <property name="idMerchant" type="int"/>
    </transfer>

    <transfer name="Store">
        <property name="idStore" type="int"/>
        <property name="name" type="string"/>
        <property name="availableCurrencyIsoCodes" type="array" singular="availableCurrencyIsoCode"/>
    </transfer>

    <transfer name="ProductManagementAttribute">
        <property name="key" type="string"/>
    </transfer>

    <transfer name="Stock">
        <property name="name" type="string"/>
    </transfer>

</transfers>
