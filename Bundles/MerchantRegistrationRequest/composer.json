{
    "name": "spryker/merchant-registration-request",
    "type": "library",
    "description": "MerchantRegistrationRequest module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/comment": "^1.9.0",
        "spryker/country": "^4.0.0",
        "spryker/gui": "^3.45.0",
        "spryker/kernel": "^3.76.0",
        "spryker/locale": "^4.0.0",
        "spryker/merchant": "^3.18.0",
        "spryker/merchant-user": "^1.6.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/store": "^1.24.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-date-time": "^1.0.0",
        "spryker/util-text": "^1.6.0",
        "spryker/util-uuid-generator": "^1.1.0",
        "spryker/zed-request": "^3.0.0"
    },
    "require-dev": {
        "spryker/application": "*",
        "spryker/code-sniffer": "*",
        "spryker/comment-user-connector": "*",
        "spryker/container": "*",
        "spryker/propel": "*",
        "spryker/testify": "*",
        "spryker/user": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
