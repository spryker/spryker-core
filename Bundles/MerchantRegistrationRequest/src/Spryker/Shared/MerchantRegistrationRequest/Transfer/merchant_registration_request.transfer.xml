<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantRegistrationRequest" strict="true">
        <property name="idMerchantRegistrationRequest" type="int"/>
        <property name="companyName" type="string"/>
        <property name="address1" type="string"/>
        <property name="address2" type="string"/>
        <property name="zipCode" type="string"/>
        <property name="city" type="string"/>
        <property name="iso2Code" type="string"/>
        <property name="registrationNumber" type="string"/>
        <property name="contactPersonTitle" type="string"/>
        <property name="contactPersonFirstName" type="string"/>
        <property name="contactPersonLastName" type="string"/>
        <property name="contactPersonRole" type="string"/>
        <property name="contactPersonPhone" type="string"/>
        <property name="email" type="string"/>
        <property name="createdAt" type="string"/>
        <property name="status" type="string"/>
        <property name="country" type="Country"/>
        <property name="store" type="Store"/>
        <property name="commentThread" type="CommentThread"/>
    </transfer>

    <transfer name="MerchantRegistrationResponse" strict="true">
        <property name="isSuccess" type="bool"/>
        <property name="errors" type="Error[]" singular="error"/>
        <property name="merchantRegistrationRequest" type="MerchantRegistrationRequest"/>
    </transfer>

    <transfer name="Error">
        <property name="message" type="string"/>
    </transfer>

    <transfer name="MerchantCriteria">
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Country">
        <property name="idCountry" type="int"/>
        <property name="name" type="string"/>
        <property name="iso2Code" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="idStore" type="int"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="CommentRequest">
        <property name="ownerId" type="int"/>
        <property name="ownerType" type="string"/>
    </transfer>

    <transfer name="UserPasswordResetRequest">
        <property name="isMerchantRegistrationRequest" type="bool"/>
        <property name="merchant" type="Merchant"/>
    </transfer>

    <transfer name="Merchant">
        <property name="merchantReference" type="string"/>
        <property name="idMerchant" type="int"/>
        <property name="urlCollection" type="Url[]" singular="url"/>
        <property name="name" type="string"/>
        <property name="email" type="string"/>
        <property name="isActive" type="bool"/>
        <property name="status" type="string"/>
        <property name="registrationNumber" type="string"/>
        <property name="merchantProfile" type="MerchantProfile"/>
        <property name="storeRelation" type="StoreRelation"/>
    </transfer>

    <transfer name="MerchantResponse">
        <property name="merchant" type="Merchant"/>
    </transfer>

    <transfer name="MerchantUser">
        <property name="idMerchant" type="int"/>
        <property name="user" type="User"/>
    </transfer>

    <transfer name="User">
        <property name="username" type="string"/>
        <property name="email" type="string"/>
        <property name="firstName" type="string"/>
        <property name="lastName" type="string"/>
    </transfer>

    <transfer name="Url">
        <property name="url" type="string"/>
        <property name="fkLocale" type="int"/>
    </transfer>

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
        <property name="localeName" type="string"/>
    </transfer>

    <transfer name="IdGeneratorSettings" strict="true">
        <property name="alphabet" type="string"/>
        <property name="size" type="int"/>
    </transfer>

    <transfer name="MerchantProfile">
        <property name="contactPersonTitle" type="string"/>
        <property name="contactPersonFirstName" type="string"/>
        <property name="contactPersonLastName" type="string"/>
        <property name="contactPersonPhone" type="string"/>
        <property name="contactPersonRole" type="string"/>
        <property name="addressCollection" type="MerchantProfileAddress[]" singular="address"/>
    </transfer>

    <transfer name="MerchantProfileAddress">
        <property name="address1" type="string"/>
        <property name="address2" type="string"/>
        <property name="city" type="string"/>
        <property name="zipCode" type="string"/>
        <property name="fkCountry" type="int"/>
        <property name="countryName" type="string"/>
        <property name="email" type="string"/>
    </transfer>

    <transfer name="StoreRelation">
        <property name="stores" type="Store[]" singular="stores"/>
    </transfer>

    <transfer name="CommentThread">
    </transfer>

    <transfer name="MerchantUserResponse">
    </transfer>

</transfers>
