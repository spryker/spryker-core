{
    "name": "spryker/merchant-relation-request",
    "type": "library",
    "description": "MerchantRelationRequest module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/company": "^1.0.0",
        "spryker/company-business-unit": "^2.11.0",
        "spryker/company-business-unit-extension": "^1.1.0",
        "spryker/company-user": "^2.17.0",
        "spryker/company-user-extension": "^1.1.0",
        "spryker/customer": "^7.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/mail": "^4.6.0",
        "spryker/mail-extension": "^1.0.0",
        "spryker/merchant": "^3.8.0",
        "spryker/merchant-relation-request-extension": "^1.0.0",
        "spryker/merchant-relationship": "^1.12.0",
        "spryker/permission": "^1.0.0",
        "spryker/permission-extension": "^1.0.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/uuid-behavior": "^1.0.0",
        "spryker/zed-request": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/company-role": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\MerchantRelationRequest\\Helper\\": "tests/SprykerTest/Shared/MerchantRelationRequest/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
