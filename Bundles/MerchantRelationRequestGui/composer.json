{
    "name": "spryker/merchant-relation-request-gui",
    "type": "library",
    "description": "MerchantRelationRequestGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker/company": "^1.0.0",
        "spryker/company-business-unit": "^2.11.0",
        "spryker/gui": "^3.53.0",
        "spryker/kernel": "^3.30.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-gui-extension": "^1.0.0",
        "spryker/merchant-relation-request": "^1.0.0",
        "spryker/symfony": "^3.5.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-date-time": "^1.0.0",
        "spryker/util-text": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker/comment-gui": "Add the module if you want to display comments."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
