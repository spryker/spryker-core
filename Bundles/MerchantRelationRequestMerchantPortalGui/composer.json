{
    "name": "spryker/merchant-relation-request-merchant-portal-gui",
    "type": "library",
    "description": "MerchantRelationRequestMerchantPortalGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/glossary": "^3.0.0",
        "spryker/gui-table": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/mail": "^4.6.0",
        "spryker/mail-extension": "^1.0.0",
        "spryker/merchant-profile-merchant-portal-gui-extension": "^1.0.0",
        "spryker/merchant-relation-request": "^1.0.0",
        "spryker/merchant-relation-request-extension": "^1.0.0",
        "spryker/merchant-relationship-merchant-portal-gui-extension": "^1.0.0",
        "spryker/merchant-user": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/translator": "^1.1.0",
        "spryker/twig": "^3.0.0",
        "spryker/util-date-time": "^1.5.0",
        "spryker/zed-ui": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
