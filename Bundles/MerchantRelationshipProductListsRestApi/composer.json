{
    "name": "spryker/merchant-relationship-product-lists-rest-api",
    "type": "library",
    "description": "MerchantRelationshipProductListsRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/customers-rest-api-extension": "^1.2.0",
        "spryker/glue-application": "^1.18.0",
        "spryker/kernel": "^3.30.0",
        "spryker/oauth-customer-connector-extension": "^1.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
