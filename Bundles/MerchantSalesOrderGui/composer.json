{
  "name": "spryker/merchant-sales-order-gui",
  "description": "MerchantSalesOrderGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/customer": "^7.0.0",
    "spryker/gui": "^3.33.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-sales-order": "^0.7.0",
    "spryker/money": "^2.0.0",
    "spryker/sales": "^11.0.0",
    "spryker/state-machine": "^2.11.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-sanitize": "^2.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
