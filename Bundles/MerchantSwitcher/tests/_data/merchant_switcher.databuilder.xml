<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="Merchant">
        <property name="merchantReference" dataBuilderRule="unique()->sentence"/>
    </transfer>

    <transfer name="ProductOffer">
        <property name="concreteSku" dataBuilderRule="unique()->sentence(5)"/>
        <property name="productOfferReference" dataBuilderRule="unique()->sentence(5)"/>
    </transfer>

    <transfer name="WishlistItem">
        <property name="merchantReference" dataBuilderRule="unique()->sentence"/>
        <property name="productOfferReference" dataBuilderRule="unique()->word"/>
    </transfer>
</transfers>
