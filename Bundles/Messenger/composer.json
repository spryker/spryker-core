{
  "name": "spryker/messenger",
  "type": "library",
  "description": "Messenger module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/application-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/messenger-extension": "^1.0.0",
    "spryker/session": "^3.0.0 || ^4.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/container": "*",
    "spryker/silex": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use ApplicationPlugin.",
    "spryker/silex": "If you want to use ServiceProvider."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
