{
  "name": "spryker/money",
  "description": "Money module",
  "license": "proprietary",
  "require": {
    "moneyphp/money": "^3.0.0",
    "spryker/currency": "^2.0.0 || ^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/store": "^1.1.0",
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/config": "^3.0.0",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/silex": "If you want to use ServiceProvider.",
    "spryker/twig": "If you want to use Money in Twig."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Config\\Module\\": "vendor/spryker/Config/tests/_support/Module",
      "Testify\\Module\\": "vendor/spryker/Testify/tests/_support/Module",
      "Transfer\\Module\\": "vendor/spryker/Transfer/tests/_support/Module"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
