{
    "name": "spryker/multi-factor-auth-merchant-portal",
    "type": "library",
    "description": "MultiFactorAuthMerchantPortal module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/kernel": "^3.52.0",
        "spryker/multi-factor-auth": "^1.5.0",
        "spryker/multi-factor-auth-extension": "^1.2.0",
        "spryker/security-merchant-portal-gui-extension": "^1.2.0",
        "spryker/session": "^4.0.0",
        "spryker/symfony": "^3.15.0",
        "spryker/transfer": "^3.27.0",
        "spryker/user": "^3.27.0",
        "spryker/zed-navigation-extension": "^1.1.0",
        "spryker/zed-ui": "^3.4.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker/container": "Use this module when you want to use the Container.",
        "spryker/router": "Use this module when you want to use the Router."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
