{
  "name": "spryker/oauth",
  "type": "library",
  "description": "Oauth module",
  "license": "proprietary",
  "require": {
    "league/oauth2-server": "^7.1",
    "php": ">=7.2",
    "spryker/config": "^3.0.0",
    "spryker/guzzle": "^2.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/oauth-extension": "^1.3.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/auth-rest-api": "*",
    "spryker/code-sniffer": "*",
    "spryker/customer": "*",
    "spryker/installer": "*",
    "spryker/oauth-revoke": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/installer": "If you want to use Installer plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
