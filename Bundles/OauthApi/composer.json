{
    "name": "spryker/oauth-api",
    "type": "library",
    "description": "OauthApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/authentication": "^1.0.0",
        "spryker/glue-application-extension": "^1.10.0",
        "spryker/kernel": "^3.58.0",
        "spryker/oauth": "^2.4.0",
        "spryker/symfony": "^3.5.0",
        "spryker/transfer": "^3.42.0",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/glue-application": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/glue-application": "ADD SUGGEST DESCRIPTION"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
