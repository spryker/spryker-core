{
    "name": "spryker/oauth-client",
    "type": "library",
    "description": "OauthClient module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker/kernel": "^3.30.0",
        "spryker/kernel-app-extension": "^1.0.0",
        "spryker/message-broker-aws-extension": "^1.0.0",
        "spryker/message-broker-extension": "^1.0.0",
        "spryker/oauth-client-extension": "^1.0.0",
        "spryker/payment-extension": "^1.1.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/oauth-dummy": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Zed\\OauthClient\\Helper\\": "tests/SprykerTest/Zed/OauthClient/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
