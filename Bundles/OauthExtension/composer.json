{
  "name": "spryker/oauth-extension",
  "type": "library",
  "description": "OauthExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "psr/http-message": "^1.0.0",
    "spryker/code-sniffer": "*",
    "spryker/oauth": "^2.0.0"
  },
  "suggest": {
    "psr/http-message": "When you want to use plugin interfaces of this module.",
    "league/oauth2-server": "When you want to use plugin interfaces of this module.",
    "spryker/oauth": "When you want to use plugin interfaces of this module."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
