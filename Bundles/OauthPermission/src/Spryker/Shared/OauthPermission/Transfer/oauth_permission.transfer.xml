<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CustomerIdentifier">
        <property name="idCompanyUser" type="string"/>
        <property name="permissions" type="PermissionCollection" deprecated="true"/>
    </transfer>

    <transfer name="CompanyUserIdentifier">
        <property name="idCompanyUser" type="string"/>
        <property name="permissions" type="PermissionCollection" deprecated="true"/>
    </transfer>

    <transfer name="PermissionCollection">
        <property name="permissions" type="Permission[]" singular="permission"/>
    </transfer>

    <transfer name="Permission">
        <property name="key" type="string"/>
        <property name="configuration" type="array" singular="configuration"/>
    </transfer>

    <transfer name="OauthAccessTokenData">
        <property name="oauthUserId" type="string"/>
    </transfer>

    <transfer name="CompanyUser">
        <property name="idCompanyUser" type="int"/>
        <property name="uuid" type="string"/>
    </transfer>

    <transfer name="Customer">
    </transfer>

    <transfer name="OauthPermissionStorageKey" strict="true">
        <property name="idCompanyUser" type="string"/>
    </transfer>
</transfers>
