{
    "name": "spryker/order-amendment-example",
    "type": "library",
    "description": "OrderAmendmentExample module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/checkout": "^6.9.0",
        "spryker/checkout-extension": "^1.4.0",
        "spryker/kernel": "^3.76.0",
        "spryker/sales": "^11.7.0",
        "spryker/sales-order-amendment": "^1.0.0",
        "spryker/sales-order-amendment-extension": "^1.1.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/oms": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/oms": "If you want to use ApplyOrderAmendmentDraftCommandByOrderPlugin plugin: ^11.46.0."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
