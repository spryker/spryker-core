<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="PaymentMethod">
        <property name="methodName" dataBuilderRule="unique()->word"/>
        <property name="name" dataBuilderRule="word"/>
        <property name="isActive" dataBuilderRule="boolean"/>
    </transfer>

    <transfer name="PaymentProvider">
        <property name="name" dataBuilderRule="word(1)"/>
        <property name="paymentProviderKey" dataBuilderRule="word(1)"/>
    </transfer>
</transfers>
