{
    "name": "spryker/payment-gui",
    "type": "library",
    "description": "PaymentGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/gui": "^3.26.0",
        "spryker/kernel": "^3.33.0",
        "spryker/payment": "^5.8.0",
        "spryker/store": "^1.9.0",
        "spryker/symfony": "^3.2.2",
        "spryker/transfer": "^3.27.0",
        "spryker/util-text": "^1.2.3"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
