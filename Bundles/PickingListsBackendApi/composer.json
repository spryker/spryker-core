{
    "name": "spryker/picking-lists-backend-api",
    "type": "library",
    "description": "PickingListsBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/glue-application-authorization-connector-extension": "^1.0.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/oauth-warehouse": "^1.0.0",
        "spryker/picking-list": "^1.0.0",
        "spryker/picking-lists-backend-api-extension": "^1.0.0",
        "spryker/symfony": "^3.5.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-encoding": "^2.0.0",
        "spryker/warehouse-user": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/glue-application": "*",
        "spryker/propel": "*",
        "spryker/stock": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use plugins with glue application."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
