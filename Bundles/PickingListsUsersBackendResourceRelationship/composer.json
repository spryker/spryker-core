{
    "name": "spryker/picking-lists-users-backend-resource-relationship",
    "type": "library",
    "description": "PickingListsUsersBackendResourceRelationship module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/picking-list": "^0.2.0",
        "spryker/transfer": "^3.31.0",
        "spryker/users-backend-api": "^1.2.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/stock": "*",
        "spryker/testify": "*",
        "spryker/user": "*",
        "spryker/warehouse-user": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
