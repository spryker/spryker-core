{
  "name": "spryker/price-cart-connector",
  "type": "library",
  "description": "PriceCartConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/price": "^5.0.0",
    "spryker/price-product": "^1.3.0 || ^2.3.0"
  },
  "require-dev": {
    "spryker/calculation": "*",
    "spryker/cart": "*",
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
