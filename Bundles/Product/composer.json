{
    "name": "spryker/product",
    "type": "library",
    "description": "Product module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/event": "^1.0.0 || ^2.3.0",
        "spryker/kernel": "^3.34.0",
        "spryker/key-builder": "^1.0.0",
        "spryker/locale": "^3.0.0 || ^4.0.0",
        "spryker/log": "^3.0.0",
        "spryker/message-broker": "^1.15.0",
        "spryker/message-broker-extension": "^1.0.0",
        "spryker/product-extension": "^1.5.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/publisher-extension": "^1.0.0",
        "spryker/storage": "^3.0.0",
        "spryker/store": "^1.1.0",
        "spryker/symfony": "^3.0.0",
        "spryker/touch": "^3.0.0 || ^4.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/url": "^3.16.0",
        "spryker/util-encoding": "^2.0.0",
        "spryker/util-text": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/product-attribute": "*",
        "spryker/product-image": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\Product\\Helper\\": "tests/SprykerTest/Shared/Product/_support/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
