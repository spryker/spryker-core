<?xml version="1.0"?>
<transfers
        xmlns="spryker:transfer-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>

    <transfer name="ProductManagementAttribute">
        <property name="idProductManagementAttribute" dataBuilderRule="="/>
        <property name="key" dataBuilderRule="unique()->text(10)"/>
        <property name="inputType" dataBuilderRule="unique()->text(10)"/>
        <property name="allowInput" dataBuilderRule="boolean(100)"/>
        <property name="isSuper" dataBuilderRule="boolean(0)"/>
        <property name="localizedKeys" dataBuilderRule="="/>
        <property name="values" dataBuilderRule="="/>
    </transfer>

    <transfer name="LocalizedProductManagementAttributeKey">
        <property name="localeName" dataBuilderRule="unique()->text(5)"/>
        <property name="keyTranslation" dataBuilderRule="="/>
    </transfer>

</transfers>
