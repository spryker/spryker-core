{
    "name": "spryker/product-attributes-backend-api",
    "type": "library",
    "description": "ProductAttributesBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/locale": "^3.0.0 || ^4.0.0",
        "spryker/product-attribute": "^1.11.0",
        "spryker/symfony": "^3.5.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/glue-application": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use glue application plugins"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
