{
    "name": "spryker/product-bundles-rest-api",
    "type": "library",
    "description": "ProductBundlesRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/glue-application": "^1.8.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/orders-rest-api": "^4.5.0",
        "spryker/orders-rest-api-extension": "^1.1.0",
        "spryker/product-bundle-storage": "^1.0.0",
        "spryker/product-storage": "^1.24.0",
        "spryker/symfony": "^3.0.0",
        "spryker/synchronization-behavior": "^1.0.0",
        "spryker/transfer": "^3.42.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
