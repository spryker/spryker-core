<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
    </transfer>

    <transfer name="ProductLabel">
        <property name="name" type="string"/>
        <property name="isActive" type="bool"/>
        <property name="validFrom" type="string"/>
        <property name="validTo" type="string"/>
        <property name="localizedAttributesCollection" singular="localizedAttributes" type="ProductLabelLocalizedAttributes[]"/>
    </transfer>

    <transfer name="StorageProductLabel">
        <property name="key" type="string"/>
    </transfer>

</transfers>
