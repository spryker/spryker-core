{
  "name": "spryker/product-management",
  "description": "ProductManagement module",
  "license": "proprietary",
  "require": {
    "everon/collection": "^1.0.0",
    "spryker/availability": "^5.0.0",
    "spryker/category": "^3.0.0",
    "spryker/currency": "^2.1.0",
    "spryker/glossary": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/price": "^4.2.0",
    "spryker/product": "^5.0.0",
    "spryker/product-attribute": "*",
    "spryker/product-bundle": "^2.0.0 || ^3.0.0",
    "spryker/product-group": "^1.0.0",
    "spryker/product-image": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/stock": "^4.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/tax": "^4.0.0 || ^5.0.0",
    "spryker/touch": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/propel": "*",
    "spryker/silex": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
