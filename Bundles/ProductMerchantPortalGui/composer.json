{
    "name": "spryker/product-merchant-portal-gui",
    "type": "library",
    "description": "ProductMerchantPortalGui module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker/category": "^4.21.0 || ^5.0.0",
        "spryker/currency": "^3.0.0",
        "spryker/decimal-object": "^1.0.0",
        "spryker/gui-table": "^0.4.4",
        "spryker/kernel": "^3.51.0",
        "spryker/laminas": "^1.0.0",
        "spryker/locale": "^3.0.0",
        "spryker/merchant-product": "^0.2.4",
        "spryker/merchant-user": "^0.5.0 || ^0.6.0 || ^0.7.0",
        "spryker/money": "^1.0.0 || ^2.0.0",
        "spryker/oms": "^11.7.0",
        "spryker/price-product": "^4.23.0",
        "spryker/product": "^6.0.0",
        "spryker/product-attribute": "^1.8.0",
        "spryker/product-category": "^4.6.0",
        "spryker/product-image": "^3.0.0",
        "spryker/product-merchant-portal-gui-extension": "^0.1.1",
        "spryker/product-validity": "^1.0.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/store": "^1.1.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.25.0",
        "spryker/translator": "^1.1.0",
        "spryker/util-encoding": "^2.0.0",
        "spryker/zed-ui": "^0.4.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
