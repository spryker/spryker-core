{
  "name": "spryker/product-offer-availability",
  "type": "library",
  "description": "ProductOfferAvailability module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/availability-extension": "^1.0.0",
    "spryker/decimal-object": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/oms": "^11.7.0",
    "spryker/product-offer-stock": "^0.3.0",
    "spryker/transfer": "^3.18.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/product-offer": "*",
    "spryker/propel": "*",
    "spryker/stock": "*",
    "spryker/store": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
