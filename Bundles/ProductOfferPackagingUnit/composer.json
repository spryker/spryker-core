{
    "name": "spryker/product-offer-packaging-unit",
    "type": "library",
    "description": "ProductOfferPackagingUnit module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/kernel": "^3.33.0",
        "spryker/oms": "^11.7.0",
        "spryker/oms-extension": "^1.2.0",
        "spryker/propel-orm": "^1.9.0",
        "spryker/sales": "^11.0.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/decimal-object": "*",
        "spryker/product": "*",
        "spryker/product-offer": "*",
        "spryker/product-packaging-unit": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
