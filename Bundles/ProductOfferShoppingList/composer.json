{
    "name": "spryker/product-offer-shopping-list",
    "type": "library",
    "description": "ProductOfferShoppingList module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/kernel": "^3.52.0",
        "spryker/product-offer": "^1.2.0",
        "spryker/shopping-list-extension": "^1.4.0",
        "spryker/store": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/product": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
