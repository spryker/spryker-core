{
    "name": "spryker/product-offer-shopping-list-data-import",
    "type": "library",
    "description": "ProductOfferShoppingListDataImport module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/data-import": "^1.6.0",
        "spryker/kernel": "^3.30.0",
        "spryker/product-offer": "^1.2.0",
        "spryker/shopping-list": "^4.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/company": "*",
        "spryker/company-user": "*",
        "spryker/container": "*",
        "spryker/customer": "*",
        "spryker/product": "*",
        "spryker/propel": "*",
        "spryker/propel-orm": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
