{
    "name": "spryker/product-offers-rest-api",
    "type": "library",
    "description": "ProductOffersRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/glue-application": "^1.0.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.33.0",
        "spryker/product-offer": "^1.0.0",
        "spryker/product-offer-storage": "^1.0.0",
        "spryker/transfer": "^3.42.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
