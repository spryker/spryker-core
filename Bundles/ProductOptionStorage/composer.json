{
  "name": "spryker/product-option-storage",
  "description": "ProductOptionStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/currency": "^3.2.0",
    "spryker/event-behavior": "^0.1.2",
    "spryker/kernel": "^3.16.0",
    "spryker/price": "^5.1.0",
    "spryker/product": "^5.5.0 || ^6.0.0",
    "spryker/product-option": "^6.1.0",
    "spryker/propel-orm": "^1.5.0",
    "spryker/storage": "^3.4.0",
    "spryker/store": "^1.6.0",
    "spryker/synchronization": "^0.2.0"
  },
  "require-dev": {
    "spryker/event": "*"
  },
  "suggest": {
    "spryker/event": "If you want to use Event plugins, minimum required version: 2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
