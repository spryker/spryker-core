{
    "name": "spryker/product-prices-rest-api",
    "type": "library",
    "description": "ProductPricesRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/currency": "^3.14.0 || ^4.0.0",
        "spryker/glue-application": "^1.8.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/price": "^5.0.0",
        "spryker/price-product": "^2.1.0 || ^4.46.0",
        "spryker/price-product-storage": "^2.3.0 || ^4.0.0",
        "spryker/product-prices-rest-api-extension": "^1.0.0",
        "spryker/product-storage": "^1.2.0",
        "spryker/products-rest-api": "^2.0.0",
        "spryker/store": "^1.4.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.42.0",
        "spryker/wishlists-rest-api-extension": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
