{
    "name": "spryker/product-relation-collector",
    "type": "library",
    "description": "ProductRelationCollector module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/collector": "^5.1.1 || ^6.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/price-product": "^1.0.0 || ^2.0.0 || ^4.0.0",
        "spryker/product": "^5.0.0 || ^6.0.0",
        "spryker/product-image": "^3.0.0",
        "spryker/product-relation": "^1.0.0 || ^2.0.0 || ^3.0.0",
        "spryker/propel-orm": "^1.1.0",
        "spryker/symfony": "^3.0.0",
        "spryker/touch": "^3.0.0 || ^4.0.0",
        "spryker/url": "^3.0.0",
        "spryker/util-data-reader": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
