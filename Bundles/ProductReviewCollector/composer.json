{
    "name": "spryker/product-review-collector",
    "type": "library",
    "description": "ProductReviewCollector module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/collector": "^5.5.0 || ^6.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/product-review": "^1.0.0 || ^2.0.0",
        "spryker/propel-orm": "^1.1.0",
        "spryker/search": "^6.0.0 || ^7.0.0 || ^8.0.0",
        "spryker/store": "^1.4.0",
        "spryker/symfony": "^3.0.0",
        "spryker/touch": "^3.0.0 || ^4.0.0",
        "spryker/transfer": "^3.25.0",
        "spryker/util-data-reader": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
