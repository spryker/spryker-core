{
    "name": "spryker/product-review-gui",
    "type": "library",
    "description": "ProductReviewGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/customer": "^6.0.0 || ^7.0.0",
        "spryker/gui": "^3.45.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.1.0 || ^4.0.0",
        "spryker/product": "^5.0.0 || ^6.0.0",
        "spryker/product-review": "^1.0.0 || ^2.0.0",
        "spryker/propel-orm": "^1.15.0",
        "spryker/symfony": "^3.0.0",
        "spryker/util-date-time": "^1.0.0",
        "spryker/util-sanitize": "^2.0.0",
        "spryker/util-text": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
