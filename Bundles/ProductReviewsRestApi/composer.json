{
    "name": "spryker/product-reviews-rest-api",
    "type": "library",
    "description": "ProductReviewsRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/glue-application": "^1.15.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/product-review": "^2.1.0",
        "spryker/product-review-storage": "^1.0.0",
        "spryker/product-storage": "^1.22.0",
        "spryker/products-rest-api-extension": "^1.1.0",
        "spryker/store": "^1.19.0",
        "spryker/symfony": "^3.0.0",
        "spryker/synchronization-behavior": "^1.0.0",
        "spryker/transfer": "^3.42.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
