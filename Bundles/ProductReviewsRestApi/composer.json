{
  "name": "spryker/product-reviews-rest-api",
  "type": "library",
  "description": "ProductReviewsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.30.0",
    "spryker/product-reviews-storage": "^1.0.0",
    "spryker/products-rest-api-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/glue-application": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/glue-application": "If you want to use RestRequestInterface.",
    "spryker/products-rest-api": "If you want to use plugins with ProductsRestApi module."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
