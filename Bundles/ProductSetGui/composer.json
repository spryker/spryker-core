{
  "name": "spryker/product-set-gui",
  "type": "library",
  "description": "ProductSetGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/gui": "^3.33.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/price-product": "^1.0.0 || ^2.0.0 || ^4.0.0",
    "spryker/product": "^5.0.0 || ^6.0.0",
    "spryker/product-image": "^3.3.0",
    "spryker/product-set": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/url": "^3.5.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/util-text": "^1.2.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
