<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductSetPageSearch">
        <property name="idProductAbstracts" type="array" singular="idProductAbstracts"/>
        <property name="idProductSet" type="int"/>
        <property name="isActive" type="bool"/>
        <property name="weight" type="int"/>
        <property name="productSetKey" type="string"/>
        <property name="name" type="string"/>
        <property name="description" type="string"/>
        <property name="metaTitle" type="string"/>
        <property name="metaKeywords" type="string"/>
        <property name="metaDescription" type="string"/>
        <property name="url" type="string"/>
        <property name="imageSets" type="array" singular="imageSets"/>
        <property name="locale" type="string"/>
        <property name="store" type="string" deprecated="Will be removed with next major release."/>
        <property name="type" type="string"/>
    </transfer>

    <transfer name="Locale">
        <property name="localeName" type="string"/>
        <property name="idLocale" type="int"/>
    </transfer>

    <transfer name="ProductImageSet">
        <property name="name" type="string"/>
        <property name="productImages" type="ProductImage[]" singular="productImage"/>
    </transfer>

    <transfer name="StorageProductImage">
        <property name="altTextSmall" type="string" strict="true"/>
        <property name="altTextLarge" type="string" strict="true"/>
    </transfer>

    <transfer name="PageMap">
        <property name="store" type="string"/>
        <property name="locale" type="string"/>
        <property name="type" type="string"/>
    </transfer>

    <transfer name="ProductImage">
    </transfer>

    <transfer name="ProductSetDataStorage">
        <property name="weight" type="int"/>
    </transfer>

    <transfer name="SearchContext">
        <property name="sourceIdentifier" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Translation">
        <property name="isActive" type="bool"/>
        <property name="glossaryKey" type="GlossaryKey" strict="true"/>
        <property name="value" type="string"/>
    </transfer>

    <transfer name="GlossaryKey">
        <property name="key" type="string"/>
    </transfer>

</transfers>
