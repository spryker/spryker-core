{
  "name": "spryker/product-tax-sets-rest-api",
  "type": "library",
  "description": "ProductTaxSetsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/glue-application": "^1.8.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.22.0",
    "spryker/symfony": "^3.0.0",
    "spryker/tax-product-storage": "^1.0.0",
    "spryker/tax-storage": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/products-rest-api": "^2.0.0",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/products-rest-api": "If you want to expand product resources with tax sets relationships.",
    "spryker/uuid": "If you want to generate UUIDs for existing entities."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
