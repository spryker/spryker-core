{
    "name": "spryker/product-tax-sets-rest-api",
    "type": "library",
    "description": "ProductTaxSetsRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/glue-application": "^1.8.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/symfony": "^3.0.0",
        "spryker/tax-product-storage": "^1.0.0",
        "spryker/tax-storage": "^1.0.0",
        "spryker/transfer": "^3.42.0",
        "spryker/uuid-behavior": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker/uuid": "If you want to generate UUIDs for existing entities."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
