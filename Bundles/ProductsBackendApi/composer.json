{
    "name": "spryker/products-backend-api",
    "type": "library",
    "description": "ProductsBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/category": "^5.10.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/locale": "^3.0.0 || ^4.0.0",
        "spryker/product": "^6.35.0",
        "spryker/product-category": "^4.17.0",
        "spryker/product-image": "^3.11.0",
        "spryker/store": "^1.12.0",
        "spryker/symfony": "^3.10.0",
        "spryker/tax": "^5.12.0",
        "spryker/transfer": "^3.42.0",
        "spryker/url": "^3.10.0",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/glue-application": "*",
        "spryker/oms": "*",
        "spryker/picking-list": "*",
        "spryker/propel": "*",
        "spryker/sales": "*",
        "spryker/stock": "*",
        "spryker/testify": "*",
        "spryker/user": "*",
        "spryker/warehouse-user": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use the Glue backend resource plugins. The minimum varsion is 1.39.0."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
