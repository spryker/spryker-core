{
    "name": "spryker/products-categories-resource-relationship",
    "type": "library",
    "description": "ProductsCategoriesResourceRelationship module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/categories-rest-api": "^1.3.0",
        "spryker/glue-application": "^1.1.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/product-category-storage": "^1.11.0 || ^2.6.0",
        "spryker/product-storage": "^1.15.0",
        "spryker/store": "^1.4.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
