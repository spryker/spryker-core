{
    "name": "spryker/push-notification-web-push-php",
    "type": "library",
    "description": "PushNotificationWebPushPhp module",
    "license": "proprietary",
    "require": {
        "minishlink/web-push": "~v8.0.0",
        "php": ">=8.0",
        "psr/http-message": "^1.0.1",
        "spryker/installer-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/push-notification": "^0.1.0",
        "spryker/push-notification-extension": "^0.1.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/guzzle": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/installer": "If you want to use Installer plugins."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
