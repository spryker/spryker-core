{
    "name": "spryker/quote-requests-rest-api",
    "type": "library",
    "description": "QuoteRequestsRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker/carts-rest-api": "^5.7.0",
        "spryker/glue-application": "^1.18.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/quote-request": "^2.0.0",
        "spryker/shipment": "^8.6.0",
        "spryker/symfony": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
