{
  "name": "spryker/redis",
  "type": "library",
  "description": "Redis module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "predis/predis": "^1.1.1",
    "spryker/kernel": "^3.30.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.8.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/web-profiler-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use Web Profiler plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
