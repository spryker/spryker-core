{
  "name": "spryker/related-products-rest-api",
  "type": "library",
  "description": "RelatedProductsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/glue-application": "^1.9.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/product-relation-storage": "^1.4.0",
    "spryker/product-storage": "^1.2.0",
    "spryker/products-rest-api": "^2.3.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
