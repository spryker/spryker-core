{
  "name": "spryker/sales",
  "description": "Sales module",
  "license": "proprietary",
  "require": {
    "spryker/calculation": "^4.0.0",
    "spryker/country": "^3.0.0",
    "spryker/customer": "^6.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/oms": "^7.0.0",
    "spryker/propel": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales-split": "^3.0.0",
    "spryker/sequence-number": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/user": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-sanitize": "^2.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/price": "*",
    "spryker/shipment": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins you need to install spryker/checkout."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\Sales\\": "tests/SprykerTest/Shared/Sales/_support/",
      "SprykerTest\\Zed\\Sales\\": "tests/SprykerTest/Zed/Sales/_support/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    },
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
