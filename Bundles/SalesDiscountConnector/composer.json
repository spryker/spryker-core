{
    "name": "spryker/sales-discount-connector",
    "type": "library",
    "description": "SalesDiscountConnector module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/discount": "^9.0.0",
        "spryker/discount-extension": "^1.2.0",
        "spryker/kernel": "^3.33.0",
        "spryker/merchant-sales-order-extension": "^1.1.0",
        "spryker/sales": "^11.50.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/customer": "*",
        "spryker/locale": "*",
        "spryker/oms": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
