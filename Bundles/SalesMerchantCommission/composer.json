{
    "name": "spryker/sales-merchant-commission",
    "type": "library",
    "description": "SalesMerchantCommission module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/acl-merchant-portal-extension": "^1.0.0",
        "spryker/calculation": "^4.0.0",
        "spryker/calculation-extension": "^1.0.0",
        "spryker/cart-extension": "^4.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/merchant-commission": "^1.0.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/refund-extension": "^1.0.0",
        "spryker/sales": "^11.49.0",
        "spryker/sales-extension": "^1.11.0",
        "spryker/sales-merchant-commission-extension": "^1.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/uuid-behavior": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/oms": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/oms": "If you want to use Oms command plugins."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\SalesMerchantCommission\\Helper\\": "tests/SprykerTest/Shared/SalesMerchantCommission/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
