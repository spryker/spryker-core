{
    "name": "spryker/sales-order-amendment",
    "type": "library",
    "description": "SalesOrderAmendment module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/availability-extension": "^1.2.0",
        "spryker/cart-extension": "^4.0.0",
        "spryker/cart-reorder-extension": "^1.0.0",
        "spryker/checkout-extension": "^1.5.0",
        "spryker/currency-extension": "^1.1.0",
        "spryker/kernel": "^3.76.0",
        "spryker/messenger": "^3.0.0",
        "spryker/persistent-cart-extension": "^1.2.0",
        "spryker/price-extension": "^1.1.0",
        "spryker/propel-orm": "^1.0.0",
        "spryker/quote": "^2.0.0",
        "spryker/quote-request-extension": "^1.1.0",
        "spryker/sales": "^11.63.0",
        "spryker/sales-extension": "^1.3.0",
        "spryker/sales-order-amendment-extension": "^1.3.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-encoding": "^2.0.0",
        "spryker/uuid-behavior": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/customer": "*",
        "spryker/oms": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "SprykerTest\\Shared\\SalesOrderAmendment\\Helper\\": "tests/SprykerTest/Shared/SalesOrderAmendment/Helper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
