{
    "name": "spryker/sales-order-amendment-oms",
    "type": "library",
    "description": "SalesOrderAmendmentOms module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/cart-reorder-extension": "^1.0.0",
        "spryker/checkout-extension": "^1.3.0",
        "spryker/kernel": "^3.76.0",
        "spryker/mail": "^4.6.0",
        "spryker/mail-extension": "^1.0.0",
        "spryker/oms": "^11.46.0",
        "spryker/quote-extension": "^1.1.0",
        "spryker/sales": "^11.51.0",
        "spryker/sales-extension": "^1.12.0",
        "spryker/sales-order-amendment": "^1.0.0",
        "spryker/sales-order-amendment-extension": "^1.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/customer": "*",
        "spryker/propel": "*",
        "spryker/store": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
