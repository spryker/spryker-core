{
  "name": "spryker/sales-product-connector",
  "type": "library",
  "description": "SalesProductConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.30.0",
    "spryker/product": "^5.0.0 || ^6.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^7.0.0 || ^8.0.0 || ^10.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/checkout": "*",
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to save product specific information during checkout"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
