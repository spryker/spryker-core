{
  "name": "spryker/sales-product-connector",
  "description": "SalesProductConnector module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/product": "^5.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^7.0.0 || ^8.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to save product specific information during checkout"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\Sales\\": "tests/SprykerTest/Shared/Sales/_support/",
      "SprykerTest\\Zed\\Sales\\": "tests/SprykerTest/Zed/Sales/_support/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    },
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
