{
  "name": "spryker/sales-reclamation-gui",
  "description": "SalesReclamationGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/sales": "^7.0.0 || ^8.0.0",
    "spryker/sales-extension": "^1.0.0",
    "spryker/sales-reclamation": "^0.0.0 || ^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/manual-order-entry-gui-extension": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/manual-order-entry-gui":"^0.5.0"
  },
  "suggest": {
    "spryker/manual-order-entry-gui": "If you want to use 'Create connected order' button"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
