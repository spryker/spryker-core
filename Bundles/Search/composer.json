{
  "name": "spryker/search",
  "type": "library",
  "description": "Search module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/config": "^3.0.0",
    "spryker/elastica": "^4.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/guzzle": "^2.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/log": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/product-page-search-extension": "^1.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "6.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
