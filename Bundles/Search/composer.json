{
  "name": "spryker/search",
  "description": "Search module",
  "license": "proprietary",
  "require": {
    "spryker/config": "^3.0.0",
    "spryker/elastica": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/log": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/propel-orm": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "6.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
