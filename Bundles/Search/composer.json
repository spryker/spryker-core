{
  "name": "spryker/search",
  "type": "library",
  "description": "Search module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/config": "^3.0.0",
    "spryker/elastica": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/guzzle": "^2.0.0",
    "spryker/health-check-extension": "^1.0.0",
    "spryker/kernel": "^3.52.0",
    "spryker/laminas": "^1.0.0",
    "spryker/log": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/product-page-search-extension": "^1.0.0",
    "spryker/search-extension": "^1.0.0",
    "spryker/store": "^1.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/search-elasticsearch": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/silex": "Add this when you still want to use the abandoned Silex."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
