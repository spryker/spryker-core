{
    "name": "spryker/search-elasticsearch-gui",
    "type": "library",
    "description": "SearchElasticsearchGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/gui": "^3.64.0",
        "spryker/kernel": "^3.30.0",
        "spryker/search-elasticsearch": "^1.2.0",
        "spryker/search-extension": "^1.0.0",
        "spryker/symfony": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/elastica": "*"
    },
    "suggest": {
        "spryker/elastica": "If you want to use Elastica plugin to build GUI."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
