<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="Template">
        <property name="idPageElementTemplate" type="int" />
        <property name="templateName" type="string" />
    </transfer>

    <transfer name="PageElement">
        <property name="idSearchPageElement" type="int" />
        <property name="elementKey" type="string" />
        <property name="isElementActive" type="bool" />
        <property name="fkSearchDocumentAttribute" type="int" />
        <property name="fkSearchPageElementTemplate" type="int" />
    </transfer>

    <transfer name="DocumentAttribute">
        <property name="idSearchDocumentAttribute" type="int" />
        <property name="attributeName" type="string" />
    </transfer>

</transfers>

