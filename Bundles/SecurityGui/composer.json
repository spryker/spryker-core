{
    "name": "spryker/security-gui",
    "type": "library",
    "description": "SecurityGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/kernel": "^3.30.0",
        "spryker/log": "^3.17.0",
        "spryker/messenger": "^3.0.0",
        "spryker/multi-factor-auth-extension": "^1.1.0",
        "spryker/security": "^1.8.0",
        "spryker/security-blocker": "^1.0.0",
        "spryker/security-extension": "^1.0.0",
        "spryker/security-gui-extension": "^1.2.0",
        "spryker/session": "^4.0.0",
        "spryker/symfony": "^3.5.0",
        "spryker/transfer": "^3.27.0",
        "spryker/user": "^3.17.0",
        "spryker/user-password-reset": "^1.0.0",
        "spryker/validator": "^1.6.0"
    },
    "require-dev": {
        "spryker/application": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/customer": "*",
        "spryker/event-dispatcher": "*",
        "spryker/form": "*",
        "spryker/propel": "*",
        "spryker/router": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker-shop/customer-page": "ADD SUGGEST DESCRIPTION",
        "spryker/router": "You need to install this module when you want to use the RouterExtension interfaces."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
