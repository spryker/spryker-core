{
    "name": "spryker/service-points-backend-api",
    "type": "library",
    "description": "ServicePointsBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/service-point": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.31.0",
        "spryker/uuid-behavior": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/config": "*",
        "spryker/container": "*",
        "spryker/glue-application": "*",
        "spryker/glue-backend-api-application": "*",
        "spryker/propel": "*",
        "spryker/storage": "*",
        "spryker/store": "*",
        "spryker/testify": "*",
        "spryker/zed-request": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use glue application plugin"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
