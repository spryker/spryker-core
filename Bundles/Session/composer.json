{
  "name": "spryker/session",
  "description": "Session bundle",
  "license": "proprietary",
  "require": {
    "spryker/config": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/new-relic-api": "^1.0.0",
    "spryker/redis": "^2.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "ClientUnit": "tests/",
      "SharedUnit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
