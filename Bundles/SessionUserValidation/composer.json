{
    "name": "spryker/session-user-validation",
    "type": "library",
    "description": "SessionUserValidation module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/container": "^1.4.0",
        "spryker/kernel": "^3.30.0",
        "spryker/security-extension": "^1.0.0",
        "spryker/session-user-validation-extension": "^1.0.0",
        "spryker/symfony": "^3.8.0",
        "spryker/transfer": "^3.27.0",
        "spryker/user": "^3.17.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker/event-dispatcher": "Add this module if you want to use security plugins."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
