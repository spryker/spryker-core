{
  "name": "spryker/shipment",
  "description": "Shipment module",
  "license": "proprietary",
  "require": {
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/money": "^2.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^6.0.0",
    "spryker/session": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/tax": "^5.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/country": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/calculation": "If you want to use Calculation plugins you need to install spryker/calculation.",
    "spryker/checkout": "If you want to use Checkout plugins you need to install spryker/checkout."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Config\\Module\\": "vendor/spryker/config/tests/_support/Module",
      "Propel\\Module\\": "vendor/spryker/propel/tests/_support/Module",
      "Transfer\\Module\\": "vendor/spryker/transfer/tests/_support/Module"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
