{
  "name": "spryker/shipment-cart-connector",
  "description": "ShipmentCartConnector module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.13.0",
    "spryker/price": "^5.0.0",
    "spryker/shipment": "^6.1.0"
  },
  "require-dev": {
    "spryker/cart": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use shipment cart plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
