{
    "name": "spryker/shipment-types-backend-api",
    "type": "library",
    "description": "ShipmentTypesBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/shipment-type": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.31.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/glue-application": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use the Glue backend resource plugins. The minimum version is 1.39.0."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
