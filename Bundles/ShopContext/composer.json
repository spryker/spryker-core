{
    "name": "spryker/shop-context",
    "type": "library",
    "description": "ShopContext module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/application-extension": "^1.0.0",
        "spryker/kernel": "^3.33.0",
        "spryker/shop-context-extension": "^0.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*"
    },
    "suggest": {
        "spryker/container": "If you want to use the Container module."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
