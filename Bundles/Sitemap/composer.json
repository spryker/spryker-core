{
    "name": "spryker/sitemap",
    "type": "library",
    "description": "Sitemap module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/file-system": "^2.0.0",
        "spryker/flysystem": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/sitemap-extension": "^1.1.0",
        "spryker/store": "^1.19.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/router": "*"
    },
    "suggest": {
        "spryker/router": "If you want to use the router plugin, you need to install the Spryker Router module."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
