{
  "name": "spryker/stock",
  "description": "Stock module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/product": "^5.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/touch": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\Stock\\": "tests/SprykerTest/Shared/Stock/_support/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/Spryker/"
    },
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
