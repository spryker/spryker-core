<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="StockProduct">
        <property name="sku" type="string" />
        <property name="stockType" type="string" />
        <property name="quantity" type="int" />
        <property name="isNeverOutOfStock" type="string" />
        <property name="idStockProduct" type="int" />
    </transfer>

    <transfer name="Type">
        <property name="idStock" type="int" />
        <property name="name" type="string" />
    </transfer>

</transfers>

