{
  "name": "spryker/storage-database",
  "type": "library",
  "description": "StorageDatabase module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.33.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/storage-database-extension": "^1.0.0",
    "spryker/storage-extension": "^1.0.0",
    "spryker/synchronization-behavior": "^1.4.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/availability": "*",
    "spryker/availability-storage": "*",
    "spryker/code-sniffer": "*",
    "spryker/product": "*",
    "spryker/product-quantity": "*",
    "spryker/product-quantity-storage": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Client\\StorageDatabase\\Helper\\": "tests/SprykerTest/Client/StorageDatabase/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
