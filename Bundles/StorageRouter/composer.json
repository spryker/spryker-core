{
  "name": "spryker/storage-router",
  "type": "library",
  "description": "StorageRouter module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/router-extension": "^1.0.0",
    "spryker/storage-router-extension": "^1.0.0",
    "spryker/symfony": "^3.2.2",
    "symfony-cmf/routing": "^2.1.0",
    "spryker/url-storage": "^1.4.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
