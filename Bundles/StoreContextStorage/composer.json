{
    "name": "spryker/store-context-storage",
    "type": "library",
    "description": "StoreContextStorage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/event-behavior": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/publisher-extension": "^1.0.0",
        "spryker/store-extension": "^1.0.0",
        "spryker/store-storage": "^1.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/queue": "*",
        "spryker/store": "*",
        "spryker/store-context": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
