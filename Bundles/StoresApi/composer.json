{
    "name": "spryker/stores-api",
    "type": "library",
    "description": "StoresApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker/application-extension": "^1.0.0",
        "spryker/country": "^3.2.0 || ^4.0.0",
        "spryker/currency": "^3.2.0 || ^4.0.0",
        "spryker/glossary-storage": "^1.0.0",
        "spryker/glue-application-extension": "^1.10.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.0.0 || ^4.0.0",
        "spryker/store": "^1.19.0",
        "spryker/store-storage": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/glue-application": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker/glue-application": "If you want to use plugins with glue application."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
