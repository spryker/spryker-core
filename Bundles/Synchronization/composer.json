{
  "name": "spryker/synchronization",
  "description": "Synchronization module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/elastica": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "spryker/kernel": "^3.4.0",
    "spryker/queue": "^1.0.0",
    "spryker/search": "^6.8.0 || ^7.0.0 || ^8.0.0",
    "spryker/storage": "^3.3.2",
    "spryker/symfony": "^3.1.0",
    "spryker/synchronization-extension": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "^3.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
