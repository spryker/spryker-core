{
  "name": "spryker/transfer",
  "description": "Transfer bundle",
  "license": "proprietary",
  "require": {
    "fzaninotto/faker": "^1.6",
    "spryker/kernel": "^3.0.0",
    "spryker/log": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "Transfer\\Module\\": "tests/_support/Module/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
