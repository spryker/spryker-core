{
  "name": "spryker/zed-navigation",
  "type": "library",
  "description": "ZedNavigation module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.30.0",
    "spryker/log": "^2.0.0 || ^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig-extension": "^1.0.0",
    "spryker/url": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use ZedNavigationTwigPlugin",
    "spryker/silex": "If you want to use ServiceProvider.",
    "spryker/twig": "If you want to use ZedNavigationServiceProvider."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\ZedNavigation\\Helper\\": "tests/SprykerTest/Zed/ZedNavigation/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
