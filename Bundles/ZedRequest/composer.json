{
  "name": "spryker/zed-request",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "require": {
    "spryker/config": "^2.0.0",
    "spryker/guzzle": "^2.1.0",
    "spryker/kernel": "^2.0.0",
    "spryker/log": "^2.0.0",
    "spryker/messenger": "^1.0.0 || ^2.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^2.1.8",
    "spryker/util-text": "^1.0.0",
    "spryker/util-network": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "description": "ZedRequest bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
