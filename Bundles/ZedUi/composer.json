{
    "name": "spryker/zed-ui",
    "type": "library",
    "description": "ZedUi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker/application-extension": "^1.0.0",
        "spryker/kernel": "^3.33.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/translator": "^1.1.0",
        "spryker/twig": "^3.13.0",
        "spryker/twig-extension": "^1.0.0",
        "spryker/util-encoding": "^2.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*"
    },
    "suggest": {
        "spryker/container": "If you want to use Twig plugins",
        "spryker/ui-components": "If you want to use Spryker web components in HTML.",
        "spryker/zed-navigation": "If you want to use the navigation."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
