<?xml version="1.0"?>
<config>
    <self-service-portal-model>
        <label>Models</label>
        <title>Models</title>
        <bundle>self-service-portal</bundle>
        <controller>list-model</controller>
        <action>index</action>
        <pages>
            <self-service-portal-model-view>
                <label>View model</label>
                <title>View model</title>
                <bundle>self-service-portal</bundle>
                <controller>view-model</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-model-view>
            <self-service-portal-model-update>
                <label>Update model</label>
                <title>Update model</title>
                <bundle>self-service-portal</bundle>
                <controller>update-model</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-model-update>
            <self-service-portal-model-attach>
                <label>Attach model</label>
                <title>Attach model</title>
                <bundle>self-service-portal</bundle>
                <controller>attach-model</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-model-attach>
        </pages>
    </self-service-portal-model>
    <self-service-portal-asset>
        <label>Assets</label>
        <title>Assets</title>
        <bundle>self-service-portal</bundle>
        <controller>list-asset</controller>
        <action>index</action>
        <pages>
            <self-service-portal-asset-update>
                <label>Update asset</label>
                <title>Update asset</title>
                <bundle>self-service-portal</bundle>
                <controller>update-asset</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-asset-update>
        </pages>
    </self-service-portal-asset>
    <self-service-portal-company-file>
        <label>File Attachments</label>
        <title>File Attachments</title>
        <bundle>self-service-portal</bundle>
        <controller>list-file</controller>
        <action>index</action>
        <pages>
            <self-service-portal-company-file-attach>
                <label>Attach File</label>
                <title>Attach File</title>
                <bundle>self-service-portal</bundle>
                <controller>attach-file</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-company-file-attach>
        </pages>
    </self-service-portal-company-file>
    <self-service-portal-inquiries>
        <label>Ssp Inquiries</label>
        <title>Ssp Inquiries</title>
        <bundle>self-service-portal</bundle>
        <controller>list-inquiry</controller>
        <action>index</action>
        <pages>
            <self-service-portal-inquiry-view>
                <label>View Inquiry</label>
                <title>View Inquiry</title>
                <bundle>self-service-portal</bundle>
                <controller>view-inquiry</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-inquiry-view>
        </pages>
    </self-service-portal-inquiries>
    <self-service-portal-services>
        <label>Booked Services</label>
        <title>Booked Services</title>
        <bundle>self-service-portal</bundle>
        <controller>list-service</controller>
        <action>index</action>
        <icon>fa-paperclip</icon>
        <pages>
            <self-service-portal-service-view>
                <label>View Service</label>
                <title>View Service</title>
                <bundle>self-service-portal</bundle>
                <controller>view-service</controller>
                <action>index</action>
                <visible>0</visible>
            </self-service-portal-service-view>
        </pages>
    </self-service-portal-services>
</config>
