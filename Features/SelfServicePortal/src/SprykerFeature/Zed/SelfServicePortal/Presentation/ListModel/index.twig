{% extends '@Gui/layout/layout.twig' %}

{% set widget_title = 'Models' | trans %}

{% block head_title widget_title %}
{% block section_title widget_title %}

{% block action %}
    {{ createActionButton(url(addModelRoute), 'Create' | trans) }}
{% endblock %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' %}
        {% block widget_title -%}
            <h5>{{ 'List of Models' | trans}}</h5>
