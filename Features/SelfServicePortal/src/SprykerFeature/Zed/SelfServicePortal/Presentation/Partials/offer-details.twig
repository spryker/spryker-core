{% block content %}
    {% set params = productConcreteTransfer.localizedAttributes[0] %}

    {% block details %}
        <div class="row">
            {% block image %}
                <div class="col-xs-3">
                    {% set defaultImage = 'data:image/webp;base64,' %}
                    {% set image = defaultImage %}
                    {% if productConcreteTransfer.getImageSets() is not empty and productConcreteTransfer.getImageSets().count() > 0 %}
                        {% set imageSets = productConcreteTransfer.getImageSets() %}
                        {% if imageSets.offsetGet(0) is defined and imageSets.offsetGet(0).productImages is not empty and imageSets.offsetGet(0).productImages.count() > 0 %}
                            {% set productImages = imageSets.offsetGet(0).productImages %}
                            {% if productImages.offsetGet(0) is defined and productImages.offsetGet(0).externalUrlSmall is not empty %}
                                {% set image = productImages.offsetGet(0).externalUrlSmall %}
                            {% endif %}
                        {% endif %}
                    {% endif %}

                    <img src="{{ image }}" class="img-thumbnail" alt="{{ params.name }}">
                </div>
            {% endblock %}

            {% block info %}
                <div class="col-xs-9">
                    {% block basicInfo %}
                        {% set data = [
                            {
                                label: 'Name',
                                value: params.name,
                            },
                            {
                                label: 'Sku',
                                value: productConcreteTransfer.sku,
                            },
                            {
                                label: 'Valid From',
                                value: productConcreteTransfer.validFrom ? productConcreteTransfer.validFrom | date('Y-m-d') : '-',
                            },
                            {
                                label: 'Valid To',
                                value: productConcreteTransfer.validTo ? productConcreteTransfer.validTo | date('Y-m-d') : '-',
                            },
                            {
                                label: 'Status',
                                value: (productConcreteTransfer.isActive ? 'Active' : 'Inactive') | trans,
                                class: "label label-#{productConcreteTransfer.isActive == 'approved' ? 'success' : 'danger'}"
                            },
                        ] %}

                        {% if productConcreteTransfer.approvalStatus %}
                            {% set data = data | merge([{
                                label: 'Approval Status',
                                value: productConcreteTransfer.approvalStatus,
                                class: "label label-#{productConcreteTransfer.approvalStatus == 'approved' ? 'success' : 'danger'}"
                            }]) %}
                        {% endif %}

                        {% block rowsInfo %}
                            {% for item in data %}
                                <div class="row spacing-bottom">
                                    <div class="col-xs-3">
                                        <strong>{{ item.label | trans }}</strong>
                                    </div>
                                    <div class="col-xs-9">
                                        <span {% if item.class | default %}class="{{ item.class }}"{% endif %}>{{ item.value | trans }}</span>
                                    </div>
                                </div>
                            {% endfor %}
                        {% endblock %}
                    {% endblock %}

                    {% block additionalInfo %}
                        {% if params.description or params.attributes %}
                            {% embed '@Gui/Partials/localized-ibox.twig' with {'collapsed': true, 'localeName': params.locale.localeName} %}
                                {% block content %}
                                    {% if params.description %}
                                        <div class="row">
                                            <div class="col-xs-2">
                                                <strong>{{ 'Description' | trans }}</strong>
                                            </div>
                                            <div class="col-xs-10">{{ params.description | raw }}</div>
                                        </div>
                                    {% endif %}

                                    {% if params.attributes | default %}
                                        {% if params.description %}
                                            <div class="hr-line-dashed"></div>
                                        {% endif %}

                                        <div class="row">
                                            <div class="col-xs-2">
                                                <strong>{{ 'Attributes' | trans }}</strong>
                                            </div>
                                            <div class="col-xs-10">
                                                <dl class="key-value">
                                                    {% for attributeName, attributeValue in params.attributes %}
                                                        <dt>{{ attributeName | trans }}:</dt>
                                                        <dd>{{ attributeValue }}</dd>
                                                    {% endfor %}
                                                </dl>
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endblock %}
                            {% endembed %}
                        {% endif %}
                    {% endblock %}
                </div>
            {% endblock %}
        </div>
    {% endblock %}

    {% block divider %}
        <div class="hr-line-dashed"></div>
    {% endblock %}
{% endblock %}
